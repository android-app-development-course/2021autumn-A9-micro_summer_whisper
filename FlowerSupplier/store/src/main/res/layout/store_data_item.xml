<?xml version="1.0" encoding="utf-8"?>
<com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="30dp"
    android:layout_marginHorizontal="40dp"
    app:qmui_backgroundColor="@color/white"
    app:qmui_radius="10dp"
    >

    <com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:qmui_radius="5dp"
        android:id="@+id/store_data_item_cons1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日访问量"
            android:id="@+id/store_data_text1_title"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/store_data_text2"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="今日访问量"
            android:id="@+id/store_data_text2_title"
            app:layout_constraintLeft_toRightOf="@id/store_data_text1_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/store_data_text3_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日订单"
            android:id="@+id/store_data_text3_title"
            app:layout_constraintLeft_toRightOf="@id/store_data_text2_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100"
            android:id="@+id/store_data_text1"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text1_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text1_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text1_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="200"
            android:id="@+id/store_data_text2"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text2_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text2_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text2_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="300"
            android:id="@+id/store_data_text3"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text3_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text3_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text3_title"
            />
    </com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout>




    <com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/store_data_item_cons1"
        android:layout_marginTop="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日访问量"
            android:id="@+id/store_data_text4_title"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/store_data_text5_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="今日访问量"
            android:id="@+id/store_data_text5_title"
            app:layout_constraintLeft_toRightOf="@id/store_data_text4_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/store_data_text6_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昨日订单"
            android:id="@+id/store_data_text6_title"
            app:layout_constraintLeft_toRightOf="@id/store_data_text5_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100"
            android:id="@+id/store_data_text4"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text4_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text4_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text4_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="200"
            android:id="@+id/store_data_text5"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text5_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text5_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text5_title"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="300"
            android:id="@+id/store_data_text6"
            app:layout_constraintLeft_toLeftOf="@id/store_data_text6_title"
            app:layout_constraintRight_toRightOf="@id/store_data_text6_title"
            app:layout_constraintTop_toBottomOf="@id/store_data_text6_title"
            />
    </com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout>


</com.micro_summer_whisper.flower_supplier.common.QMUIRoundConstraintLayout>